{
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "crlf",
    "objectWrap": "preserve",
    "printWidth": 100,
    "useTabs": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": false,
    "singleAttributePerLine": true,
    "semi": true,
    "quoteProps": "as-needed",
    "proseWrap": "preserve"
}